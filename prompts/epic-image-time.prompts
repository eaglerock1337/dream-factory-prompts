[config]

!MODE = random							# random mode; queue random prompts from [prompts] sections below
!SD_LOW_MEMORY = no					# low GPU VRAM mode (yes/no)? slower but far less VRAM required
!SD_LOW_MEM_TURBO = yes				# if you're still getting out-of-memory errors in low VRAM mode, set this to no
!DELIM = " "							# delimiter to use between prompt sections, default is space
!WIDTH = 512							# output image width, default is 512
!HEIGHT = 512							# output image height, default is 512
!STEPS = 250							# number of steps, more may improve image but increase generation time
!SAMPLER = plms                       # sampler to use (plms, ddim, lms, euler, euler_a, dpm2, dpm2_a, heun)
!SAMPLES = 1					      	# number of images to generate per prompt
!MIN_SCALE = 7.5						# minimum guidance scale, default = 7.5
!MAX_SCALE = 7.5						# maximum guidance scale, set min and max to same number for no variance
!RANDOM_INPUT_IMAGE_DIR =				# specify a directory of images here to randomly use them as inputs
!MIN_STRENGTH = 0.75					# min strength of starting image influence, (0-1, 1 is lowest influence)
!MAX_STRENGTH = 0.75					# max strength of start image, set min and max to same number for no variance
!CKPT_FILE = models/ldm/stable-diffusion-v1/model-ema.ckpt

# optional integrated upscaling

!USE_UPSCALE = yes						# use ESRGAN to upscale output images?
!UPSCALE_AMOUNT = 2.0					# upscaling factor
!UPSCALE_FACE_ENH = yes				# use GFPGAN to attempt to enhance faces (may make some images blurry/worse)?
!UPSCALE_KEEP_ORG = yes				# keep the original non-upscaled image (yes/no)?

# optional negative prompt

!NEG_PROMPT =

